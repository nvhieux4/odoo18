
<odoo>
    <data noupdate="1">
        <record id="ir_cron_bot_script" model="ir.cron">
            <field name="name">Execute Bot Script</field>
            <field name="model_id" ref="model_onex_bot_script_config"/>
            <field name="state">code</field>
            <field name="code">model.execute_script()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="active">True</field>
        </record>
    </data>
</odoo>
